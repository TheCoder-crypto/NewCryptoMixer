generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  

}



model Family {
  leaf      String @id @default(uuid())
  sibling1  String
  sibling2  String
  sibling3  String
  sibling4  String
  sibling5  String
  sibling6  String
  sibling7  String
}
