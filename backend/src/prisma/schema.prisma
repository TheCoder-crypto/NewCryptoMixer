generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  

}



model Family {
  leaf      BigInt @id @default(autoincrement())

  sibling1  BigInt
  sibling2  BigInt
  sibling3  BigInt
  sibling4  BigInt
  sibling5  BigInt
  sibling6  BigInt
  sibling7  BigInt
}
